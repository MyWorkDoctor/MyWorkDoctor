.row(ng-init="prevUrl='/patientStage1';nxtUrl='/patientStage3';pageNo=2")
    .col-xs-12
        h3 What part of your body is injured?
.row(ng-repeat="item in injuryPartsList") 
    input.pull-left.injury-type(type='checkbox', name="{{item.value}}", id="{{item.value}}", checkbox-group, checked-list="injuryPartsCheckedList", item="item")
    label.injury-multi-select(for='{{item.value}}', ng-bind="item.value")
.row
     .col-xs-12
        h4 Others :
        textarea.other-injury-types(ng-model="otherInjuredParts")
include ../includes/footer